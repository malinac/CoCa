{

  "manifest_version": 2,
  "name": "Coca",
  "version": "1.0",

  "description": "Calendar widget",
  "homepage_url": "https://github.com/malinac/CoCa",
  "icons": {
    "48": "icons/calendar-512.png"
  },

  "permissions": [
    "activeTab", "sessions", "storage", "notifications"
  ],

  "browser_action": {
    "default_icon": "icons/calendar-512.png",
    "default_title": "CoCa",
    "default_popup": "popup/coca.html"
  }


}